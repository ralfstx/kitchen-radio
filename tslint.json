{
  "extends": ["tslint:recommended"],
  "defaultSeverity": "error",
  "rules": {
    "arrow-parens": false,
    "curly": false,
    "forin": false,
    "indent": {
      "options": ["spaces", 2]
    },
    "interface-name": false,
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "prefer-const": false,
    "quotemark": {
      "options": ["single", "avoid-template"]
    },
    "radix": false,
    "trailing-comma": {
      "options": {
        "multiline": "never",
        "singleline": "never"
      }
    },
    "variable-name": {
      "options": "allow-leading-underscore"
    }
  }
}
